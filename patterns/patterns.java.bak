class Patterns {
    public static void main(String[] args) {
       // System.out.println("Try programiz.pro");
       int n=5;
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<n;j++)
            {
                System.out.print("*");
				//if skip the space after last column of every row
			    if(j!=n-1)
                System.out.print("_");
            }
            System.out.println();
        }
    }
}

